/* pages/list/list.wxss */
page{
  background: #f5f5f5;

}



.container{
  display: flex;
  flex-flow: row;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color:#939393;
  border-top: 1px solid #dedede;
  margin-top: 30rpx;
}


.nav_left{
   width: 25%;
   height: 100%;
   background: #f5f5f5;
   text-align: center;
   
}

.nav_left .nav_left_items{
   height: 40rpx;
   line-height: 40rpx;
   padding: 20rpx 0;
   border-bottom: 1px solid #dedede;
   font-size: 30rpx;

}
.nav_left .nav_left_items.active{
  background: #fff
}



.nav_right{
  width: 75%;
}


.nav_right_container{
  display: flex;
  flex-flow: row wrap;

}

.nav_right .nav_right_items{
  display: flex;
  width: 33.33%;
  height: 300rpx;
  text-align: center;


}

.nav_right .nav_right_container .nav_right_items text{
  font-size: 30rpx;
  margin-top: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: block;
  width: 200rpx;
}


.nav_right .nav_right_items image{
  width: 200rpx;
  height: 200rpx;
}